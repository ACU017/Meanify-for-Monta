{
    "name": "Meanify",
    "version": "0.0.1",
    "manifest_version": 2,
    "icons": {
        "16": "./images/icon-16.png",
        "48": "./images/icon-48.png",
        "128": "./images/icon-128.png"
    },
    "browser_action": {
        "default_popup": "html/popup.html",
        "default_icon": {
            "16": "./images/icon2-16.png",
            "48": "./images/icon2-48.png",
            "128": "./images/icon2-128.png"
        },
        "default_title": "Meanify"
    },
    "background": {
        "page": "html/background.html",
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "js/myscript.js"
            ]
        }
    ],
    "options_page": "./html/test.html",
    "permissions": [
        "activeTab",
        "storage",
        "contextMenus",
        "http://*/*",
        "https://*/*",
        "tabs"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.jsdelivr.net https://kit.fontawesome.com;  object-src 'self'"
}